

    <!-- cover -->
    <section class="p-0 mb-0">
      <div class="swiper-container text-white"
        data-top-top="transform: translateY(0px);"
        data-top-bottom="transform: translateY(250px);">
        <div class="swiper-wrapper">
          <div class="swiper-slide vh-100">
            <div class="image image-overlay image-zoom" style="background-image:url(/assets/img/4FriendsTopCity3.jpg)"></div>
            <div class="caption" data-aos="fade-up">
              <div class="container justify-content-center">
                <div class="row align-items-center vh-100">
                  <div class="col-lg-8" data-swiper-parallax-y="-100%">
                  <br><br><br><br><br><br><br><br><br><br>
                    <h1 class="display-4" >Travel differently in <br><i class="font-weight-bold" id="typed"></i></h1>
                    <div id="typed-strings">
                      <% @cities.each do |city| %>
                        <p><%= city.name %>.</p>
                      <% end %>
                    </div>
                    <% if user_signed_in? %>
                      <%= link_to "Welcome #{current_user.first_name}", user_path(current_user.id), class: "btn btn-primary btn-rounded px-3" %>
                    <% else %>
                      <%= link_to "Come Aboard, It's free", new_user_session_path, class: "btn btn-primary btn-rounded px-3" %>
                    <% end %>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="swiper-footer mb-5">
            <div class="container-fluid">
              <div class="row">
                <div class="col text-center">
                  <div class="mouse"></div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
     <!-- cover -->

     <!-- presentation -->
    <section class="section-lg bg-primary">
      <div class="container">
        <div class="row text-center text-lg-left text-white">
          <div class="col-12 col-lg-9">
            <div class="row">
              <div class="col-lg-8">
                <h2>For us, discovering a culture with a local is <b>the best experience ever!</b>.</h2>
              </div>
            </div>
            <div class="row gutter-0 bg-blue">
              <div class="col-sm-6 col-lg-4" data-aos="fade-up" data-aos-anchor-placement="bottom-bottom">
                <div class="bordered rising p-3">
                <i class="fas fa-user-friends fa-4x text-pink"></i>
                  <h4 class="mb-0">Friends</h4>
                  <p>Match with people from other cities with common interests.</p>
                </div>
              </div>
              <div class="col-sm-6 col-lg-4" data-aos="fade-up" data-aos-anchor-placement="bottom-bottom" data-aos-delay="150">
                <div class="bordered rising p-3">
                   <i class="fas fa-comments fa-4x text-pink"></i>
                  <h4 class="mb-0">Chat</h4>
                  <p>Talk with him, get friend and plan an exchange</p>
                </div>
              </div>
               <div class="col-sm-6 col-lg-4" data-aos="fade-up" data-aos-anchor-placement="bottom-bottom" data-aos-delay="300">
                <div class="bordered rising p-3">
                  <i class="fas fa-plane-departure fa-4x text-pink"></i>
                  <h4 class="mb-0">Travel</h4>
                  <p>Welcome him to your home and visit him !</p>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-3 presentation presentation-responsive">
            <%=image_tag("img/miffer.png", :alt => "Image", class: "left-25 vertical-align")%>          
          </div>
        </div>
      </div>
    </section>
    <!-- / presentation -->

    <!-- destination carroussel -->

     <section class="bg-light">

      <div class="row justify-content-center">
         <div class="container gallery">
        <div class="row justify-content-center">
          <div class="col-md-6 text-center">
            <h2><b>Discover</b> our cities.</h2>
            <p>Select the city of your dreams and discover all the miffers who live there.</p>
          </div>
        </div>
        <ul class="masonry row gutter-2">
              <% @cities.each do |city|%>
              <li class="col-md-6 col-lg-3">
                <% if city.name == "Paris" %>
                  <article class="tile">
                <% end %>
                <% if city.id.even? %>
                  <article class="tile tile-long">
                <% else %>
                  <% if city.id % 5 == 0 %>
                    <article class="tile tile-long">
                  <% end %>
                  <article class="tile">
                <% end %>
                  <div class="tile-image" style="background-image: url('<%=asset_path("cities/#{city.name.downcase}.jpg")%>');"></div>
                    <%= link_to city_path(city) do%>
                    <div class="tile-header">
                      <h2 class="text-white" style="text-align: center"><%= city.name %></h2>
                      <h4 class="text-white" style="text-align: center"><strong><%= city.users.count %> miffers </strong></h4>
                    </div>
                    <% end %>
                </article>
              </li>
              <% end %>
            </ul>
            <div class="row">
          <div class="col text-center">
            <%= link_to cities_path, class:"btn btn-rounded btn-with-ico btn-primary" do %>
              View All Cities <i class="icon-chevron-right2 fs-24"></i>
            <%end%>
          </div>
        </div>
          </div>
        </div>
      </div>
    </section>
    <!-- / destination carrousel -->

